{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "homework activity",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mundo75/Google-Book-Search.git"
  },
  "author": "Jeff Raimundo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mundo75/Google-Book-Search/issues"
  },
  "homepage": "https://github.com/Mundo75/Google-Book-Search#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.4",
    "nodejs-latest": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
